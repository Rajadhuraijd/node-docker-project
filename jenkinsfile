pipeline{
    agent any
    stages {
        stage("checkout scm"){
            steps{
                checkout scm
            }
        }
        stage("test"){
            steps{
                sh 'npm install express --save'
                sh 'npm test'
            }
        }
        stage("Build"){
            steps{
                sh 'npm run build'
            }
        }
  }
}
